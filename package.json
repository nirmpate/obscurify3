{
  "name": "obscurify3",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:dev": "ng build --configuration=develop",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run obscurify3:serve-ssr",
    "serve:ssr": "node dist/obscurify3/server/main.js",
    "build:ssr": "ng build --prod && ng run obscurify3:server:production",
    "build:ssr:dev": "ng build --configuration=develop && ng run obscurify3:server:develop",
    "build:ssr:prod": "ng build --prod && ng run obscurify3:server:production",
    "prerender": "npx angular-prerender --exclude-routes /authorized",
    "scully": "scully",
    "scully:serve": "scully serve",
    "buildscully": "npm run build --prod && npm run scully -- --scanRoutes --showGuessError --np --prod --host=0.0.0.0 --serverTimeout=30000",
    "buildscully:prod": "npm run build -- --prod --stats-json && npm run scully -- --scanRoutes --showGuessError",
    "scanscully": "npm run scully -- --scanRoutes --showGuessError"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.8",
    "@angular/cdk": "~10.1.2",
    "@angular/common": "~10.0.8",
    "@angular/compiler": "~10.0.8",
    "@angular/core": "~10.0.8",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "~10.0.8",
    "@angular/material": "^10.1.2",
    "@angular/platform-browser": "~10.0.8",
    "@angular/platform-browser-dynamic": "~10.0.8",
    "@angular/platform-server": "~10.0.8",
    "@angular/router": "~10.0.8",
    "@angular/service-worker": "~10.0.8",
    "@nguniversal/express-engine": "^9.1.1",
    "@scullyio/init": "^1.0.1",
    "@scullyio/ng-lib": "^1.0.3-ppack",
    "@scullyio/scully": "^1.0.0",
    "chart.js": "^2.9.3",
    "chartjs-plugin-annotation": "^0.5.7",
    "intersection-observer": "^0.10.0",
    "ngx-cookie-service": "^3.0.4",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular/cli": "^10.0.5",
    "@angular/compiler-cli": "~10.0.8",
    "@angular/language-service": "~10.0.8",
    "@nguniversal/builders": "^9.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  }
}
